{
  "name": "Javadoc Search Frame Extension",
  "version": "1.0",
  "description": "Javadoc incremental search for packages and classes.",
  "background_page": "background.html",
  "options_page": "options.html",
  "icons": {
     "16": "icons/icon16.png",
     "32": "icons/icon32.png",
     "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "content_scripts": [
    {
      "matches": [
          "http://*/*/allclasses-frame.html",
          "file://*/*/allclasses-frame.html",
          "http://*/*/package-frame.html",
          "file://*/*/package-frame.html"
      ],
      "js": [
          "lib/Frames.js",
          "lib/Log.js",
          "lib/Storage.js",
          "lib/UserPreference.js",
          "lib/OptionsPage.js",
          "allclasses-frame.js"
      ]
    },
    {
      "matches": [
          "http://*/*",
          "file://*/*"
      ],
      "js": [
          "lib/Storage.js",
          "lib/UserPreference.js",
          "top.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ]
}
