{
  "name": "Javadoc Search Frame",
  "version": "1.0",
  "description": "Incremental search frame for Javadoc packages and classes.",
  "background_page": "background.html",
  "options_page": "options.html",
  "content_scripts": [
    {
      "matches": [
          "http://*/*/allclasses-frame.html",
          "file://*/*/allclasses-frame.html",
          "http://*/*/package-frame.html",
          "file://*/*/package-frame.html"
      ],
      "js": [
          "Frames.js",
          "Log.js",
          "Storage.js",
          "UserPreference.js",
          "OptionsPage.js",
          "allclasses-frame.js"
      ]
    },
    {
      "matches": [
          "http://*/*",
          "file://*/*"
      ],
      "js": [
          "Storage.js",
          "UserPreference.js",
          "top.js"
      ]
    }
  ],
  "permissions": [
    "tabs"
  ]
}
